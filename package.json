{
  "name": "@b9/react-native-ui-components",
  "version": "1.4.1",
  "description": "React Native UI Components",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-ui-components.podspec",
    "!lib/typescript/example",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/B9Ingenieria/react-native-ui-components.git",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "author": "B9 Ingenier√≠a <daniela@b9.cl> (https://www.b9.cl)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/B9Ingenieria/react-native-ui-components/issues"
  },
  "homepage": "https://github.com/B9Ingenieria/react-native-ui-components#readme",
  "devDependencies": {
    "@commitlint/config-conventional": "^8.3.4",
    "@react-native-community/bob": "^0.16.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-native-community/toolbar-android": "^0.1.0-rc.2",
    "@react-native-picker/picker": "1.16.1",
    "@release-it/conventional-changelog": "^1.1.4",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/commit-analyzer": "^9.0.1",
    "@semantic-release/npm": "^8.0.0",
    "@semantic-release/release-notes-generator": "^10.0.2",
    "@testing-library/jest-native": "^3.4.3",
    "@testing-library/react-native": "^7.1.0",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.165",
    "@types/react": "^16.9.19",
    "@types/react-native": "0.63.2",
    "commitlint": "^8.3.5",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "16.13.1",
    "react-native": "0.63.2",
    "react-native-elements": "^3.0.0-alpha.1",
    "react-native-gesture-handler": "~1.7.0",
    "react-native-vector-icons": "^6.7.0",
    "react-test-renderer": "^17.0.1",
    "release-it": "^13.5.8",
    "semantic-release": "^18.0.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "@b9/react-native-autocomplete-search": "*",
    "@expo/vector-icons": "*",
    "@react-native-community/datetimepicker": "*",
    "@react-native-community/picker": "*",
    "@react-native-community/toolbar-android": "*",
    "@react-native-picker/picker": "*",
    "lodash": "*",
    "moment": "*",
    "react": "*",
    "react-datepicker": "*",
    "react-native": "*",
    "react-native-elements": "*",
    "react-native-gesture-handler": "*",
    "react-native-vector-icons": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint && yarn typescript"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "dependencies": {
    "@b9/react-native-autocomplete-search": "^1.3.0",
    "@react-native-community/datetimepicker": "^3.0.8",
    "date-fns": "^2.16.1",
    "global": "^4.4.0",
    "moment": "^2.29.1",
    "react-datepicker": "^3.3.0"
  }
}
